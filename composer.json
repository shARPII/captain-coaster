{
    "name": "captain-coaster/captain-coaster",
    "license": "MIT",
    "type": "project",
    "description": "Participative guide to discover, rate and review roller coasters",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "api-platform/core": "^3.0",
        "aws/aws-sdk-php": "^3.263",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.9",
        "easycorp/easyadmin-bundle": "^4",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/jsrouting-bundle": "^3.0",
        "http-interop/http-factory-guzzle": "*",
        "knplabs/knp-paginator-bundle": "^6",
        "knplabs/knp-time-bundle": "^2",
        "knpuniversity/oauth2-client-bundle": "^2.15",
        "league/flysystem-aws-s3-v3": "^3",
        "league/oauth2-facebook": "^2",
        "league/oauth2-google": "^4",
        "liip/imagine-bundle": "^2.0",
        "oneup/flysystem-bundle": "^4",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/discord-notifier": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2.0",
        "symfony/form": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfonycasts/verify-email-bundle": "^1.16",
        "twig/cssinliner-extra": "*",
        "twig/extra-bundle": "*",
        "twig/inky-extra": "*",
        "twig/intl-extra": "^3.6"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^10.0",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.3.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan -c phpstan.neon"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    }
}
